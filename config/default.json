
{

  "version": "1.0",
  "description": "Base configuration for Boeing sensor tracking simulation",

  "scenario": {
    "mode": "3D",
    "num_targets": 1,
    "duration_s": 10
  },

  "degradation": {
    "enabled": false,
    "type": "rain",
    "description": "Weather/environmental degradation mode"
  },

  "data_logging": {
    "use_saved_dataLog": false,
    "dataLog_file": "cache/myRun1.mat",
    "save_after_generation": true
  },

  "tracker_global": {
    "max_num_tracks": 500,
    "volume": 1e9,
    "beta": 1e-14,
    "detection_probability": {
        "ideal": 0.9,
        "degraded": 0.7
    }
  },

  "tracker_params": {
    "ideal": {
      "gate": 45,
      "far_gnn": 1e-6,
      "far_mht": 1e-6,
      "far_jpda": 1e-6,
      "confirm_threshold": 20,
      "delete_threshold": -5,
      "tomht_threshold_multiplier": [0.2, 1, 1],
      "max_branches": 5,
      "beta_jpda": 1e-14,
      "gate_jpda": 45,
      "time_tolerance_jpda": 0.05,
      "num_tracks_jpda": 500
    },

    "degraded": {
      "gate": 35,
      "far_gnn": 1e-2,
      "far_mht": 1e-3,
      "far_jpda": 1e-3,
      "confirm_threshold": 30,
      "delete_threshold": -15,
      "tomht_threshold_multiplier": [0.2, 5, 5],
      "max_branches": 3,
      "beta_jpda": 1e-11,
      "gate_jpda": 45,
      "time_tolerance_jpda": 0.05,
      "num_tracks_jpda": 200
    }
  },
  
  "trackers_to_run": {
    "gnn_cv": true,
    "gnn_imm": true,
    "tomht_cv": true,
    "tomht_imm": true,
    "jpda_cv": true,
    "jpda_imm": true
  },
  
  "output": {
    "print_diagnostics": true,
    "plot_initial_scenario": true,
    "save_results": true,
    "results_directory": "results"
  }

}